                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "activity": {
                                            "type": "string",
                                            "example": "Learn a new programming language"
                                        },
                                        "type": {
                                            "type": "string",
                                            "example": "education"
                                        },
                                        "participants": {
                                            "type": "integer",
                                            "example": 1
                                        },
                                        "price": {
                                            "type": "number",
                                            "format": "float",
                                            "example": 0.1
                                        },
                                        "link": {
                                            "type": "string",
                                            "example": ""
                                        },
                                        "key": {
                                            "type": "string",
                                            "example": "5881028"
                                        },
                                        "accessibility": {
                                            "type": "number",
                                            "format": "float",
                                            "example": 0.2
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
